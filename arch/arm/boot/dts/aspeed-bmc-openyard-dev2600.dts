&i2c5 {
	//Need GPIOS5 as "0"
	status = "okay";
	/* PSU1 @ 0xB0 */
	power-supply@58 {
		compatible = "inspur,ipsps1";
		reg = <0x58>;
	};

	/* PSU2 @ 0xB2 */
	/*power-supply@51 {
		compatible = "inspur,ipsps1";
		reg = <0x51>;
	};*/
};
//
&i2c6 {
	//Need ina219 [80,82,88]
	status = "okay";
	Current_Meter_GPU@40 {
		compatible = "ti,ina219";
		reg = <0x40>;
		shunt-resistor = <1000>; /* = 1 mOhm */
	};
	Current_Meter_GPU@41 {
		compatible = "ti,ina219";
		reg = <0x41>;
		shunt-resistor = <1000>; /* = 1 mOhm */
	};
	Current_Meter_GPU@44 {
		compatible = "ti,ina219";
		reg = <0x44>;
		shunt-resistor = <1000>; /* = 1 mOhm */
	};
};
