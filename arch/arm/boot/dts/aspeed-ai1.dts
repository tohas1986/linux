	leds {
		compatible = "gpio-leds";

		heartbeat {
			label = "heartbeat";
			gpios = <&gpio ASPEED_GPIO(A, 0) GPIO_ACTIVE_LOW>;
			linux,default-trigger = "timer";
			active-delay = <100>;
			inactive-delay = <100>;
		};

		status_green {
			default-state = "keep";
			gpios = <&gpio ASPEED_GPIO(A, 2) GPIO_ACTIVE_LOW>;
		};

		status_amber {
			default-state = "keep";
			gpios = <&fp_gpio 8 GPIO_ACTIVE_HIGH>;
		};

		identify {
			default-state = "keep";
			gpios = <&fp_gpio 9 GPIO_ACTIVE_HIGH>;
