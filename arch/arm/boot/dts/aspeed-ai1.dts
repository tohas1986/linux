		#size-cells = <0>;
		reg = <0x70>;
		i2c-mux-idle-disconnect;
		i2c_nvme5: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};
		i2c_nvme6: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};
	};
	switch_m2 {
		compatible = "nxp,pca9546";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x76>;
		i2c-mux-idle-disconnect;
		i2c_m2_0: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};
		i2c_m2_1: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};
	};
	sata_nct7601 {
		compatible = "nuvoton,nct7601";
		reg = <0x1d>;
		#address-cells = <1>;
		#size-cells = <0>;
	};
};

