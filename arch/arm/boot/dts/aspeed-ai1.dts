		i2c43 = &i2c_sas3;
		i2c44 = &i2c_sas4;
		i2c28 = &i2c_m2_0;
		i2c29 = &i2c_m2_1;
		i2c31 = &i2c_slimpcie_5_6;
		i2c32 = &i2c_slimpcie_1_2;
