	status = "okay";
};

&i2c2 {
	bus-frequency = <100000>;
	aspeed,dma-buf-size = <4095>;
	aspeed,hw-timeout-ms = <300>;
	multi-master;
	status = "okay";
	ipmb@10 {
		compatible = "ipmb-dev";
		reg = <0x10>;
		i2c-protocol;
	};
};

&i2c3 {
	bus-frequency = <100000>;
	aspeed,dma-buf-size = <4095>;
	aspeed,hw-timeout-ms = <300>;
	status = "okay";

	fru@54 {
		compatible = "atmel,24c08";
		reg = <0x54>;
	};

	switch_slimpcie {
		compatible = "nxp,pca9546";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x71>;
		i2c-mux-idle-disconnect;
		i2c_slimpcie_1_2: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
			switch_gpur {
				compatible = "nxp,pca9546";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x70>;
				i2c-mux-idle-disconnect;
				i2c_pcie_slot1: i2c@0 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0>;
				};
				i2c_pcie_slot2: i2c@1 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <1>;
					ina219@40 {
						compatible = "ti,ina219";
						reg = <0x40>;
						shunt-resistor = <1100>; //1000uOhm + 100uOhm (loss compensation)
					};
				};
				i2c_pcie_slot3: i2c@2 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <2>;
					ina219@44 {
						compatible = "ti,ina219";
						reg = <0x44>;
						shunt-resistor = <1200>; //1200uOhm + 200uOhm (loss compensation)
					};
				};
				i2c_pcie_slot4: i2c@3 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <3>;
				};
			};
		};
		i2c_slimpcie_3_4: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};
		i2c_slimpcie_5_6: i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
		};
		i2c_slimpcie_7_8: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;
		};
	};
};

&i2c4 {
	bus-frequency = <100000>;
	aspeed,dma-buf-size = <4095>;
	aspeed,hw-timeout-ms = <300>;
	status = "okay";

	inlet_temp {
		compatible = "ti,tmp75";
		reg = <0x4b>;
	};

	fp_gpio: fp_gpio@20 {
		compatible = "nxp,pca9555";
		reg = <0x20>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupt-parent = <&gpio>;
		interrupts = <ASPEED_GPIO(A, 3) IRQ_TYPE_EDGE_FALLING>;
		gpio-line-names =
			"FP_SW_1","FP_SW_2","RESET_BUTTON","ID_BUTTON","IO04","IO05","IO06","IO07",
			"IO10","IO11","IO12","IO13","IO14","IO15","IO16","IO17";
	};
};

