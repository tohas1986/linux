&kcs3 {
	kcs_addr = <0xCA2>;
	aspeed,lpc-io-reg = <0xca2>;
	status = "okay";
};

&kcs4 {
	kcs_addr = <0xCA4>;
	aspeed,lpc-io-reg = <0xca4>;
	status = "okay";
};

&lpc_sio {
	status = "okay";
};


&lpc_snoop {
	snoop-ports = <0x80>;
	status = "okay";
};

&mbox {
	status = "okay";
};

/**
 * BIOS flash connected via MUX controlled by GPIO R.3 (BIOS_SEL_N)
*/
&spi1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_spi1_default>;

	flash@0 {
		m25p,fast-read;
		status = "okay";
	};
};

&mac0 {
	status = "disabled";
};

&mac1 {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_rgmii2_default &pinctrl_mdio2_default>;
};

&i2c0 {
	bus-frequency = <100000>;
	general-call;
	aspeed,dma-buf-size = <4095>;
	aspeed,hw-timeout-ms = <300>;
	status = "okay";
	switch_sashd {
		compatible = "nxp,pca9548";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x73>;
		i2c-mux-idle-disconnect;
		i2c_sas4: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};
		i2c_sas3: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};
		i2c_sas2: i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
		};
		i2c_sas1: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;
		};
	};

};

&i2c1 {
	bus-frequency = <100000>;
	aspeed,dma-buf-size = <4095>;
	aspeed,hw-timeout-ms = <300>;
	status = "okay";

	i2c-switch@73 {
		compatible = "nxp,pca9548";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x73>;
		i2c-mux-idle-disconnect;

		i2c_sw73_ch0: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};

		i2c_sw73_ch1: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;

			fru@0x53 {
				compatible = "atmel,24c256";
				reg = <0x53>;
			};

			i2c-switch_1@70 {
				compatible = "nxp,pca9545";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x70>;
				i2c-mux-idle-disconnect;

				i2c_gen_z_right: i2c@0 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0>;
				};
				i2c_sw70_ch1: i2c@1 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <1>;
				};
				i2c_sw70_ch2: i2c@2 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <2>;
				};
				i2c_sw70_ch3: i2c@3 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <3>;

					tmp75@4c {
						compatible = "ti,tmp75";
						reg = <0x4c>;
					};
				};
			};

			i2c-switch_1@71 {
				compatible = "nxp,pca9545";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x71>;
				i2c-mux-idle-disconnect;

				i2c_sw_ch0_gpu0: i2c@0 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0>;
				};

				i2c_sw_ch1_gpu1: i2c@1 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <1>;
				};

				i2c_sw_ch2_gpu2: i2c@2 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <2>;
				};
			};
		};

		i2c_sw73_ch2: i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;

				i2c-switch_1@70 {
					compatible = "nxp,pca9545";
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0x70>;
					i2c-mux-idle-disconnect;

					i2c_gen_z_left: i2c@0 {
						#address-cells = <1>;
						#size-cells = <0>;
						reg = <0>;
					};
				};
		};

		i2c_sw73_ch3: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;

				i2c-switch_3@70 {
					compatible = "nxp,pca9548";
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0x70>;
					i2c-mux-idle-disconnect;

						i2c_m2_0: i2c@0 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <0>;
						};
						i2c_m2_1: i2c@1 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <1>;
						};
						i2c_ina219: i2c@2 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <2>;

							ina219@40 {
								compatible = "ti,ina219";
								shunt-resistor = <600>; //500uOhm + 100uOhm (loss compensation)
								reg = <0x40>;
							};
						};
						i2c_clk_buf: i2c@3 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <3>;
						};
						i2c_m2_lm5066: i2c@4 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <4>;
						};
						i2c_channel5: i2c@5 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <5>;
						};
						i2c_channel6: i2c@6 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <6>;
						};
						i2c_channel7: i2c@7 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <7>;
							nct7802-m2@28 {
								compatible = "nuvoton,nct7802";
								reg = <0x28>;
								#address-cells = <1>;
								#size-cells = <0>;
								channel@1 {
									reg = <1>;
									sensor-type = "temperature";
									temperature-mode = "thermistor";
								};
								channel@2 {
									reg = <2>;
									sensor-type = "temperature";
									temperature-mode = "thermistor";
								};
							};
						};
					};
			};
		};
};

&i2c2 {
	bus-frequency = <100000>;
	aspeed,dma-buf-size = <4095>;
	aspeed,hw-timeout-ms = <300>;
	multi-master;
	status = "okay";
	ipmb@10 {
		compatible = "ipmb-dev";
		reg = <0x10>;
		i2c-protocol;
	};
};

&i2c3 {
	bus-frequency = <100000>;
	aspeed,dma-buf-size = <4095>;
	aspeed,hw-timeout-ms = <300>;
	status = "okay";


	i2c-switch@72 {
		compatible = "nxp,pca9545";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x72>;
		i2c-mux-idle-disconnect;

		i2c_12v: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};
		i2c_48v: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};
	};
};

&i2c4 {
	bus-frequency = <100000>;
	aspeed,dma-buf-size = <4095>;
	aspeed,hw-timeout-ms = <300>;
	status = "okay";

	tmp75@4b {
		compatible = "ti,tmp75";
		reg = <0x4b>;
	};

	tmp75@4c {
		compatible = "ti,tmp75";
		reg = <0x4c>;
	};

	tmp75@4d {
		compatible = "ti,tmp75";
		reg = <0x4d>;
	};

	tmp75@4f {
		compatible = "ti,tmp75";
		reg = <0x4f>;
	};
	i2c-switch@71 {
		compatible = "nxp,pca9548";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x71>;
		i2c-mux-idle-disconnect;
		interrupt-controller;
		#interrupt-cells = <2>;

		i2c_cpld: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};
	};
};

