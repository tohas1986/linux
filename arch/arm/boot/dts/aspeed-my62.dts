&i2c11 {
	bus-frequency = <100000>;
	aspeed,dma-buf-size = <4095>;
	aspeed,hw-timeout-ms = <300>;
	status = "okay";

		i2c-switch@70 {
				compatible = "nxp,pca9548";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x70>;
				i2c-mux-idle-disconnect;

				i2c_nvme0: i2c@0 {
						#address-cells = <1>;
						#size-cells = <0>;
						reg = <0>;
				};

				i2c_nvme1: i2c@1 {
						#address-cells = <1>;
						#size-cells = <0>;
						reg = <1>;
				};

				i2c_nvme2: i2c@2 {
						#address-cells = <1>;
						#size-cells = <0>;
						reg = <2>;
				};

				i2c_nvme3: i2c@3 {
						#address-cells = <1>;
						#size-cells = <0>;
						reg = <3>;
				};
				i2c_nvme4: i2c@4 {
						#address-cells = <1>;
						#size-cells = <0>;
						reg = <4>;
				};
				i2c_nvme5: i2c@5 {
						#address-cells = <1>;
						#size-cells = <0>;
						reg = <5>;
				};
		};
};

